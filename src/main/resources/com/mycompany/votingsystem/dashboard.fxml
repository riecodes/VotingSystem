<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.votingsystem.DashboardController">
    <children>
        <Label fx:id="welcomeLabel" text="Welcome!" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
        <HBox alignment="CENTER" spacing="15.0">
            <children>
                <Button fx:id="voteButton" text="Vote" onAction="#goToVote" />
                <Button fx:id="resultsButton" text="Results" onAction="#goToResults" />
                <Button fx:id="candidateButton" text="Candidate Management" onAction="#goToCandidate" />
                <Button fx:id="auditLogButton" text="Audit Log" onAction="#goToAuditLog" />
                <Button fx:id="logoutButton" text="Logout" onAction="#logout" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
    </padding>
</VBox> 